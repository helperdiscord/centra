{
	"extends": ["@kludge-cs/eslint-config-js", "plugin:react/recommended"],
	"env": {
		"es2021": true,
		"browser": true
	},
	"parserOptions": {
		"ecmaVersion": 12,
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"settings": {
		"react": {
			"version": "17"
		}
	},
	"rules": {
		"node/no-unsupported-features/es-syntax": 0
	}
}
